{
  "name": "sommelier-ai",
  "version": "0.0.0",
  "packageManager": "pnpm@9.11.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "node ace test",
    "test:unit": "node ace test unit"
  },
  "imports": {
    "#auth/*": "./app/auth/*.js",
    "#wine/*": "./app/wine/*.js",
    "#taster/*": "./app/taster/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#mails/*": "./app/mails/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#fixtures/*": "./database/fixtures/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.7.28",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.16.6",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "hot-hook": "^0.2.6",
    "pino-pretty": "^11.2.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.7"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.3",
    "@adonisjs/core": "^6.14.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/inertia": "^1.2.1",
    "@adonisjs/lucid": "^21.2.0",
    "@adonisjs/session": "^7.4.2",
    "@adonisjs/shield": "^8.1.1",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^3.0.0",
    "@heroicons/vue": "^2.1.5",
    "@inertiajs/vue3": "^1.2.0",
    "@vinejs/vine": "^2.1.0",
    "@vue/server-renderer": "^3.5.8",
    "edge.js": "^6.2.0",
    "luxon": "^3.5.0",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "vue": "^3.5.8"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config"
}
